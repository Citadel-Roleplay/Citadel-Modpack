### WALLS 01
pdxmesh = {
	name = "walls_01_sea_mesh"
	file = "walls_01_sea.mesh"
	meshsettings = {
		name = "walls_01_sea"
		index = 0
		texture_diffuse = "southeast_asian_atlas_diffuse.dds"
		texture_normal = "southeast_asian_atlas_normal.dds"
		texture_specular = "southeast_asian_atlas_properties.dds"
		texture = { file = "walls_01_sea_unique.dds" index = 5 }
		shader = "snap_to_terrain_atlas"
		shader_file = "gfx/FX/pdxmesh.shader"
	}
	meshsettings = {
		name = "decal"
		index = 0
		texture_diffuse = "western_walls_01_decal_diffuse.dds"
		texture_normal = "western_walls_01_decal_normal.dds"
		texture_specular = "western_walls_01_decal_properties.dds"
		shader = "decal_local"
		shader_file = "gfx/FX/pdxmesh_decal.shader"
		subpass = "LocalDecals"
	}
}
entity = { name = walls_01_sea pdxmesh = walls_01_sea_mesh }


