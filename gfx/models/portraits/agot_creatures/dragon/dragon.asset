skeletal_animation_set = {
    name = "dragon_animations"
    reference_skeleton = "dragon_mesh"

	animation = { 			id = "dragon_idle_flair_lookingleft" 		type = "dragon_idle_flair_lookingleft.anim" } # 300 frames
	animation = { 			id = "dragon_idle_1" 						type = "dragon_idle_1.anim" } # 120 frames
	animation = { 			id = "dragon_flying_1" 						type = "dragon_glide.anim" } # 300 frames
	animation = { 			id = "dragon_flying_2" 						type = "dragon_glide_wing_flap.anim" } # 75 frames
	animation = { 			id = "dragon_roar_1" 						type = "dragon_roar.anim" } # 300 frames
	#animation = { 			id = "dragon_roar_1" 						type = "dragon_idle_flair_lookingleft.anim" } # 300 frames
	animation = { 			id = "dragon_hover_1" 						type = "dragon_hover_1.anim" } # 60 frames

}	



